{
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "src"  
      }
    }
  ],
  "routes": [
    {
      "src": "/scripts/(.*)",
      "dest": "/src/dist/scripts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
