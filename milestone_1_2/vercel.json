{
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "src/dist"  
      }
    }
  ],
  "routes": [
    {
      "src": "^/scripts/(.*)",   
      "dest": "/dist/scripts/$1"
    },
    {
      "src": "/(.*)",            
      "dest": "/$1"
    }
  ]
}
